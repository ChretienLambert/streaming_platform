{
  "name": "streaming-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "latest",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^6.0.0",
    "@fastify/multipart": "^9.0.0",
    "@fastify/static": "^7.0.0",
    "argon2": "^0.30.0",
    "fastify": "^4.25.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "prisma": "latest",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "@types/node": "^20.4.2",
    "eslint": "^8.48.0",
    "prettier": "^3.9.1"
  }
}
